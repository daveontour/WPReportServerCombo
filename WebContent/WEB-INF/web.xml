<?xml version="1.0" encoding="UTF-8"?>
<web-app metadata-complete="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <servlet>
    <servlet-name>reportServlet</servlet-name>
    <servlet-class>au.com.quaysystems.qrm.server.ServletReportProcessor</servlet-class>
  </servlet>
    <servlet-mapping>
    <servlet-name>reportServlet</servlet-name>
    <url-pattern>/report</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>userMessageManager</servlet-name>
    <servlet-class>au.com.quaysystems.qrm.server.ServletUserMessageManager</servlet-class>
    <async-supported>true</async-supported>
  </servlet>
    <servlet-mapping>
    <servlet-name>userMessageManager</servlet-name>
    <url-pattern>/reportMsg</url-pattern>
  </servlet-mapping>
  
    <listener>
	    <listener-class>au.com.quaysystems.qrm.server.ServletListenerConcurrentManager</listener-class>
    </listener>
</web-app>